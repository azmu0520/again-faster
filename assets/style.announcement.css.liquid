@font-face {
  font-family: 'Oswald-Light';
  src: url(' {{ 'Oswald-Light.ttf' | asset_url }} ') format('truetype');
}

 

.bar-main{
background-color: #FBFBFB;
color: #000000;
}
.bar-secondary{
  background-color: #a8a1a1b2;
  color: #FBFBFB;
};
.announcement {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 40px;
  background: #fbfbfb;
  font-weight: 300;
  font-size: 12px;
  line-height: 168%;
  text-align: center;
  letter-spacing: 0.16em;
  text-transform: uppercase;
  color: #000000;
  font-family: 'Oswald-Light';
  position: fixed;
  top: 0px;
  
  
}

.announcement a {
  text-decoration: none;
  color: inherit;
}
